  shorttitle = {Iron {{Deficiency}} and {{Sleep}}/{{Wake Behaviors}}},
  author = {McWilliams, Scout and Hill, Olivia and Ipsiroglu, Osman S. and Clemens, Stefan and Weber, Alexander Mark and Chen, Michael and Connor, James and Felt, Barbara T. and Manconi, Mauro and Mattman, Andre and Silvestri, Rosalia and Simakajornboon, Narong and Smith, Susan M. and Stockler, Sylvia},
  year = 2024,
