  langid = {english},
  keywords = {Brain,Dynamical systems,Network models,Neuronal development},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Frana_et_al_2024_Neonatal_brain_dynamic_functional_connectivity_in_term_and_preterm_infants_and.pdf}
