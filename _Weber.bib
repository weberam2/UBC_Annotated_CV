@article{yoxallMeasurementCerebralOxygen1998,
  title = {Measurement of Cerebral Oxygen Consumption in the Human Neonate Using near Infrared Spectroscopy: Cerebral Oxygen Consumption Increases with Advancing Gestational Age},
  shorttitle = {Measurement of Cerebral Oxygen Consumption in the Human Neonate Using near Infrared Spectroscopy},
  author = {Yoxall, C. W. and Weindling, A. M.},
