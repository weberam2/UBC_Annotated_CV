@article{wikstromEarlySinglechannelAEEG2012,
  title = {Early Single-channel {{aEEG}}/{{EEG}} Predicts Outcome in Very Preterm Infants},
  author = {Wikstr{\"o}m, Sverre and Pupp, Ingrid Hansen and Ros{\'e}n, Ingmar and Norman, Elisabeth and Fellman, Vineta and Ley, David and Hellstr{\"o}m-Westas, Lena},
  year = 2012,
  month = jul,
  journal = {Acta Paediatrica},
  volume = {101},
  number = {7},
  pages = {719--726},
  issn = {0803-5253, 1651-2227},
  doi = {10.1111/j.1651-2227.2012.02677.x},
  urldate = {2025-08-13},
  abstract = {Abstract                            Aim:\enspace{}               To characterize early amplitude-integrated electroencephalogram (aEEG) and single-channel EEG (aEEG/EEG) in very preterm (VPT) infants for prediction of long-term outcome.                                         Patients:\enspace{}               Forty-nine infants with median (range) gestational age of 25 (22--30) weeks.                                         Methods:\enspace{}               Amplitude-integrated electroencephalogram/EEG recorded during the first 72{$\quad$}h and analysed over 0--12, 12--24, 24--48 and 48--72{$\quad$}h, for background pattern, sleep--wake cycling, seizures, interburst intervals (IBI) and interburst percentage (IB\%). In total, 2614{$\quad$}h of single-channel EEG examined for seizures. Survivors were assessed at 2{$\quad$}years corrected age with a neurological examination and Bayley Scales of Infant Development-II. Poor outcome was defined as death or survival with neurodevelopmental impairment. Good outcome was defined as survival without impairment.                                         Results:\enspace{}               Thirty infants had good outcome. Poor outcome (n{$\quad$}={$\quad$}19) was associated with depressed aEEG/EEG already during the first 12{$\quad$}h (p{$\quad$}={$\quad$}0.023), and with prolonged IBI and higher IB\% at 24{$\quad$}h. Seizures were present in 43\% of the infants and associated with intraventricular haemorrhages but not with outcome. Best predictors of poor outcome were burst-suppression pattern [76\% correctly predicted; positive predictive value (PPV) 63\%, negative predictive value (NPV) 91\%], IBI{$\quad>\quad$}6{$\quad$}sec (74\% correctly predicted; PPV 67\%, NPV 79\%) and IB\%{$\quad>\quad$}55\% at 24{$\quad$}h age (79\% correctly predicted; PPV 72\%, NPV 80\%). In 35 infants with normal cerebral ultrasound during the first 3{$\quad$}days, outcome was correctly predicted in 82\% by IB\% (PPV 82\%, NPV 83\%).                                         Conclusion:\enspace{}               Long-term outcome can be predicted by aEEG/EEG with 75--80\% accuracy already at 24 postnatal hours in VPT infants, also in infants with no early indication of brain injury.},
  langid = {english},
  file = {/home/weberam2/Zotero/storage/8KNZA2WZ/Wikstrm et al. - 2012 - Early singlechannel aEEGEEG predicts outcome in very preterm infants.pdf}
}

