  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Shew_et_al_2011_Information_capacity_and_transmission_are_maximized_in_balanced_cortical.pdf}
}

@article{shiDemyelinationRationalTherapeutic2015,
