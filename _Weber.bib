  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Lundberg_et_al_2022_Comparison_of_MRI_methods_for_measuring_wholebrain_oxygen_extraction_fraction.pdf}
