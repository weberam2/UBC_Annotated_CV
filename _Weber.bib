@article{levinSerialMRINeurobehavioural1992,
  title = {Serial {{MRI}} and Neurobehavioural Findings after Mild to Moderate Closed Head Injury},
  author = {Levin, H. S. and Williams, D. H. and Eisenberg, H. M. and High, W. M. and Guinto, F. C.},
  year = 1992,
