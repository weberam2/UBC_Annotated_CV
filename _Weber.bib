@article{tingAssociationAntibioticUtilization2018,
  title = {Association of {{Antibiotic Utilization}} and {{Neurodevelopmental Outcomes}} among {{Extremely Low Gestational Age Neonates}} without {{Proven Sepsis}} or {{Necrotizing Enterocolitis}}},
  author = {Ting, Joseph and Synnes, Anne and Roberts, Ashley and Deshpandey, Akhil and Dow, Kimberly and Yang, Junmin and Lee, Kyong-Soon and Lee, Shoo and Shah, Prakesh and {on behalf of the Canadian Neonatal Network and Canadian Neonatal Follow-Up Network}},
