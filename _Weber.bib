  author = {Griffanti, Ludovica and Douaud, Gwena{\"e}lle and Bijsterbosch, Janine and Evangelisti, Stefania and {Alfaro-Almagro}, Fidel and Glasser, Matthew F. and Duff, Eugene P. and Fitzgibbon, Sean and Westphal, Robert and Carone, Davide and Beckmann, Christian F. and Smith, Stephen M.},
