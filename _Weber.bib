@article{mccrearyMultiexponentialT2Magnetization2009,
  title = {Multiexponential {{T2}} and Magnetization Transfer {{MRI}} of Demyelination and Remyelination in Murine Spinal Cord},
  author = {McCreary, Cheryl R. and Bjarnason, Thorarin A. and Skihar, Viktor and Mitchell, J. Ross and Yong, V. Wee and Dunn, Jeff F.},
  year = 2009,
  month = may,
  journal = {NeuroImage},
  volume = {45},
  number = {4},
  pages = {1173--1182},
  issn = {1095-9572},
  doi = {10.1016/j.neuroimage.2008.12.071},
  abstract = {Identification of remyelination is important in the evaluation of potential treatments of demyelinating diseases such as multiple sclerosis. Local injection of lysolecithin into the brain or spinal cord provides a murine model of demyelination with spontaneous remyelination. The aim of this study was to determine if quantitative, multicomponent T(2) (qT(2)) analysis and magnetization transfer ratio (MTR), both indicative of myelin content, could detect changes in myelination, particularly remyelination, of the cervical spinal cord in mice treated with lysolecithin. We found that the myelin water fraction and geometric mean T(2) value of the intra/extracellular water significantly decreased at 14 days then returned to control levels by 28 days after injury, corresponding to clearance of myelin debris and remyelination which was shown by eriochrome cyanine and oil red O staining of histological sections. The MTR was significantly decreased 14 days after lysolecithin injection, and remained low over the time course studied. Evidence of demyelination shown by both qT(2) and MTR lagged behind the histological evidence of demyelination. Myelin water fraction increased with remyelination, however MTR remained lower after 28 days. The difference between qT(2) and MTR may identify early remyelination.},
  langid = {english},
  pmcid = {PMC2669308},
  pmid = {19349232},
  keywords = {Animals,Demyelinating Diseases,Image Enhancement,Image Interpretation Computer-Assisted,Lysophosphatidylcholines,Magnetic Resonance Imaging,Male,Mice,Mice Inbred C57BL,Nerve Fibers Myelinated,Nerve Regeneration,Reproducibility of Results,Sensitivity and Specificity,Spinal Cord},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/McCreary_et_al_2009_Multiexponential_T2_and_magnetization_transfer_MRI_of_demyelination_and.pdf}
