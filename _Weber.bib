@article{leeSensitivityMRIResonance2010,
  title = {Sensitivity of {{MRI}} Resonance Frequency to the Orientation of Brain Tissue Microstructure},
  author = {Lee, Jongho and Shmueli, Karin and Fukunaga, Masaki and {van Gelderen}, Peter and Merkle, Hellmut and Silva, Afonso C. and Duyn, Jeff H.},
