  author = {Smyrni, Nikoletta and Koutsaki, Maria and Petra, Marianna and Nikaina, Eirini and Gontika, Maria and Strataki, Helen and Davora, Fotini and Bouza, Helen and Damianos, George and Skouteli, Helen and Mastroyianni, Sotiria and Dalivigka, Zoi and Dinopoulos, Argyris and Tzaki, Margarita and Papavasiliou, Antigone},
