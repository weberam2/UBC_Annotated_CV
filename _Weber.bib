  keywords = {Algorithms,Anisotropy,Basal Ganglia,Body Temperature,Brain,Cadaver,Corpus Callosum,Electromagnetic Fields,Humans,Iron,Magnetic Resonance Imaging,Nerve Fibers,Signal-To-Noise Ratio,Temperature},
