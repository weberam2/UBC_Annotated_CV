  pages = {863--867},
  issn = {1522-2594},
  doi = {10.1002/mrm.23282},
  abstract = {Quantification of blood oxygen saturation on the basis of a measurement of its magnetic susceptibility demands knowledge of the difference in volume susceptibility between fully oxygenated and fully deoxygenated blood ({$\Delta\chi$}(do) ). However, two very different values of {$\Delta\chi$}(do) are currently in use. In this work we measured {$\Delta\chi$}(do) as well as the susceptibility of oxygenated blood relative to water, {$\Delta\chi$}(oxy) , by MR susceptometry in samples of freshly drawn human blood oxygenated to various levels, from 6 to 98\% as determined by blood gas analysis. Regression analysis yielded 0.273 \textpm{} 0.006 and -0.008 \textpm{} 0.003 ppm (cgs) respectively, for {$\Delta\chi$}(do) and {$\Delta\chi$}(oxy) , in excellent agreement with previous work by Spees et al. (Magn Reson Med 2001;45:533-542).},
  langid = {english},
  pmcid = {PMC3307833},
  pmid = {22162033},
  keywords = {Algorithms,Blood Physiological Phenomena,Blood Volume,Humans,Image Interpretation Computer-Assisted,Magnetic Fields,Magnetic Resonance Imaging,Oximetry,Oxygen,Reproducibility of Results,Sensitivity and Specificity},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Jain_et_al_2012_Investigating_the_magnetic_susceptibility_properties_of_fresh_human_blood_for.pdf}
