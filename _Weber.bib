@article{liCharacterizationT2Heterogeneity2009,
  title = {Characterization of {{T}}(2)* Heterogeneity in Human Brain White Matter},
  author = {Li, Tie-Qiang and Yao, Bing and {van Gelderen}, Peter and Merkle, Hellmut and Dodd, Stephen and Talagala, Lalith and Koretsky, Alan P. and Duyn, Jeff},
  year = 2009,
  month = dec,
  journal = {Magnetic Resonance in Medicine},
  volume = {62},
