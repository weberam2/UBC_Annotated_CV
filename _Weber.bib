@article{jainMRIEstimationGlobal2010,
  title = {{{MRI}} Estimation of Global Brain Oxygen Consumption Rate},
  author = {Jain, Varsha and Langham, Michael C. and Wehrli, Felix W.},
  year = 2010,
  month = sep,
  journal = {Journal of Cerebral Blood Flow and Metabolism: Official Journal of the International Society of Cerebral Blood Flow and Metabolism},
  volume = {30},
  number = {9},
  pages = {1598--1607},
  issn = {1559-7016},
  doi = {10.1038/jcbfm.2010.49},
  abstract = {Measuring the global cerebral metabolic rate of oxygen (CMRO(2)) is a valuable tool for assessing brain vitality and function. Measurement of blood oxygen saturation (HbO(2)) and flow in the major cerebral outflow and inflow vessels can provide a global estimate of CMRO(2). We demonstrate a rapid noninvasive method for quantifying CMRO(2) by simultaneously measuring venous oxygen saturation in the superior sagittal sinus with magnetic resonance susceptometry-based oximetry, a technique that exploits the intrinsic susceptibility of deoxygenated hemoglobin, and the average blood inflow rate with phase-contrast magnetic resonance imaging. The average venous HbO(2), cerebral blood flow, and global CMRO(2) values in eight healthy, normal study subjects were 64\%+/-4\%, 45.2+/-3.2 mL per 100 g per minute, and 127+/-7 micromol per 100 g per minute, respectively. These values are in good agreement with those reported in literature. The technique described is noninvasive, robust, and reproducible for in vivo applications, making it ideal for use in clinical settings for assessing the pathologies associated with dysregulation of cerebral metabolism. In addition, the short acquisition time (approximately 30 seconds) makes the technique suitable for studying the temporal variations in CMRO(2) in response to physiologic challenges.},
  langid = {english},
  pmcid = {PMC2949253},
  pmid = {20407465},
  keywords = {Adult,Algorithms,Brain Chemistry,Carotid Artery Internal,Cerebrovascular Circulation,Data Interpretation Statistical,Humans,Image Processing Computer-Assisted,Kinetics,Magnetic Resonance Imaging,Male,Oximetry,Oxygen Consumption,Phantoms Imaging,Vertebral Artery},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Jain_et_al_2010_MRI_estimation_of_global_brain_oxygen_consumption_rate.pdf}
}

@article{jainRapidMagneticResonance2011,
  title = {Rapid Magnetic Resonance Measurement of Global Cerebral Metabolic Rate of Oxygen Consumption in Humans during Rest and Hypercapnia},
  author = {Jain, Varsha and Langham, Michael C and Floyd, Thomas F and Jain, Gaurav and Magland, Jeremy F and Wehrli, Felix W},
  year = 2011,
  month = jul,
  journal = {Journal of Cerebral Blood Flow \& Metabolism},
  volume = {31},
  number = {7},
  pages = {1504--1512},
  publisher = {SAGE Publications Ltd STM},
  issn = {0271-678X},
  doi = {10.1038/jcbfm.2011.34},
  urldate = {2024-09-26},
  abstract = {The effect of hypercapnia on cerebral metabolic rate of oxygen consumption (CMRO2) has been a subject of intensive investigation and debate. Most applications of hypercapnia are based on the assumption that a mild increase in partial pressure of carbon dioxide has negligible effect on cerebral metabolism. In this study, we sought to further investigate the vascular and metabolic effects of hypercapnia by simultaneously measuring global venous oxygen saturation (SvO2) and total cerebral blood flow (tCBF), with a temporal resolution of 30 seconds using magnetic resonance susceptometry and phase-contrast techniques in 10 healthy awake adults. While significant increases in SvO2 and tCBF were observed during hypercapnia (P {$<$} 0.005), no change in CMRO2 was noted (P {$>$} 0.05). Additionally, fractional changes in tCBF and end-tidal carbon dioxide (R2 = 0.72, P {$<$} 0.005), as well as baseline SvO2 and tCBF (R2 = 0.72, P {$<$} 0.005), were found to be correlated. The data also suggested a correlation between cerebral vascular reactivity (CVR) and baseline tCBF (R2 = 0.44, P = 0.052). A CVR value of 6.1\% \textpm{} 1.6\%/mm Hg was determined using a linear-fit model. Additionally, an average undershoot of 6.7\% \textpm{} 4\% and 17.1\% \textpm{} 7\% was observed in SvO2 and tCBF upon recovery from hypercapnia in six subjects.},
  langid = {english},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Jain_et_al_2011_Rapid_magnetic_resonance_measurement_of_global_cerebral_metabolic_rate_of.pdf}
}

